@prefix dcterms:    <http://purl.org/dc/terms/> .
@prefix rdf:        <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:       <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:       <http://www.w3.org/2004/02/skos/core#> . 
@prefix owl:        <http://www.w3.org/2002/07/owl#> .
@prefix xsd:        <http://www.w3.org/2001/XMLSchema#> .
@prefix adms:       <http://www.w3.org/ns/adms#> .

#Own namespaces
@prefix lex:        <http://purl.org/lex#> .
@prefix judikat:    <http://purl.org/lex/judikat#> .
@prefix doc-kinds:  <http://purl.org/lex/document-kinds#> .
@prefix agenda:     <http://purl.org/lex/agenda-soudu#> .


##############################################################
########################## Classes ###########################
##############################################################


judikat:Spisová-značka a owl:Class ;
  rdfs:label "Soudní spisová značka"@cs ;
  rdfs:subClassOf adms:Identifier .

judikat:ECLI a owl:Class ;
  rdfs:label "Identifikátor ECLI"@cs ;
  rdfs:subClassOf adms:Identifier .
  
judikat:Číslo-jednací a owl:Class ;
  rdfs:label "Soudní číslo jednací"@cs ;
  rdfs:subClassOf adms:Identifier .

##############################################################
################ Properties - Identifiers ####################
##############################################################

judikat:číslo-senátu a owl:FunctionalProperty ;
  rdfs:label "Číslo senátu ve spisové značce"@cs ;
  rdfs:comment "Může být např. 22, 34, ... ale i I., IV. nebo Pl."@cs ;
  rdfs:domain [ a owl:Class ;
    owl:unionOf (judikat:Spisová-značka judikat:ECLI judikat:Číslo-jednací)
    ] ;
  rdfs:range xsd:string
  .

judikat:staré-číslo-senátu a owl:FunctionalProperty ;
  rdfs:label "Číslo původního senátu ve spisové značce v případě předání věci mezi senáty"@cs ;
  rdfs:comment "Pokud byla věc převedena mezi dvěma odděleními, zapisují se staré i nové číslo oddělená lomítkem, např. 12/31 Cm 441/1997"@cs ;
  rdfs:domain [ a owl:Class ;
    owl:unionOf (judikat:Spisová-značka judikat:ECLI judikat:Číslo-jednací)
    ] ;
  rdfs:range xsd:string
  .

judikat:druh-věci a owl:FunctionalProperty ;
  rdfs:label "Druh věci ve spisové značce"@cs ;
  rdfs:domain [ a owl:Class ;
    owl:unionOf (judikat:Spisová-značka judikat:ECLI judikat:Číslo-jednací)
    ] ;
  rdfs:range skos:Concept, [
    a owl:Restriction ;
    owl:onProperty skos:inScheme ;
    owl:hasValue agenda:
    ]
  .

judikat:pořadí-v-rejstříku a owl:FunctionalProperty ;
  rdfs:label "Pořadí v rejstříku ve spisové značce"@cs ;
  rdfs:domain [ a owl:Class ;
    owl:unionOf (judikat:Spisová-značka judikat:ECLI judikat:Číslo-jednací)
    ] ;
  rdfs:range xsd:integer
  .

judikat:rok a owl:FunctionalProperty ;
  rdfs:label "Rok ve spisové značce"@cs ;
  rdfs:domain [ a owl:Class ;
    owl:unionOf (judikat:Spisová-značka judikat:ECLI judikat:Číslo-jednací)
    ] ;
  rdfs:range xsd:gYear
  .

judikat:kód-země a owl:FunctionalProperty ;
  rdfs:label "Kód země v ECLI"@cs ;
  rdfs:domain judikat:ECLI ;
  rdfs:range xsd:string
  .

judikat:kód-soudu a owl:FunctionalProperty ;
  rdfs:label "Kód soudu v ECLI"@cs ;
  rdfs:domain judikat:ECLI ;
  rdfs:range xsd:string
  .

judikat:rok-ECLI a owl:FunctionalProperty ;
  rdfs:label "Rok v ECLI"@cs ;
  rdfs:domain judikat:ECLI ;
  rdfs:range xsd:gYear
  .

judikat:pořadí-rozhodnutí a owl:FunctionalProperty ;
  rdfs:label "Pořadí rozhodnutí ve stejné spisové značce"@cs ;
  rdfs:domain judikat:ECLI ;
  rdfs:range xsd:integer
  .

judikat:číslo-strany a owl:FunctionalProperty ;
  rdfs:label "Číslo strany ve spisu se spisovou značkou"@cs ;
  rdfs:domain judikat:Číslo-jednací ;
  rdfs:range xsd:integer
  .
  
######################################################  
################## Properties - judikát ##############
######################################################

judikat:typ a owl:FunctionalProperty ;
  rdfs:domain judikat:Judikát ; 
  rdfs:range skos:Concept, [
    a owl:Restriction ;
    owl:onProperty skos:inScheme ;
    owl:hasValue doc-kinds: 
    ]
  .  

judikat:dotčený-předpis a rdf:Property ;
  rdfs:label "Dotčený předpis"@cs ;
  rdfs:domain judikat:Judikát 
  .  

judikat:spisová-značka a owl:FunctionalProperty ;
  rdfs:label "Místo vydání"@cs ;
  rdfs:domain judikat:Judikát ;
  rdfs:range judikat:Spisová-značka 
  .

judikat:ecli a owl:FunctionalProperty ;
  rdfs:label "Místo vydání"@cs ;
  rdfs:domain judikat:Judikát ;
  rdfs:range judikat:ECLI 
  .

judikat:číslo-jednací a owl:FunctionalProperty ;
  rdfs:label "Číslo jednací"@cs ;
  rdfs:domain judikat:Judikát ;
  rdfs:range judikat:Číslo-jednací 
  .
  
judikat:heslo a rdf:Property ;
  rdfs:label "Heslo"@cs ;
  rdfs:domain judikat:Judikát
  .
  
judikat:místo-vydání a owl:FunctionalProperty ;
  rdfs:label "Místo vydání judikátu"@cs ;
  rdfs:domain judikat:Judikát ;
  rdfs:range xsd:string
  .
